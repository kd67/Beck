            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:elevation="2dp"
                android:background="#fff"
                android:clickable="true"
                android:focusable="true"
                android:onClick="missionInfo">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?attr/selectableItemBackground"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:id="@+id/arrow1"
                        android:src="@drawable/larrow"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentEnd="false"
                        android:layout_centerHorizontal="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Our Mission"
                        android:id="@+id/ourmission"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentRight="false"
                        android:layout_centerInParent="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center"

                        />

                </LinearLayout>
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:background="?attr/colorPrimary" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:elevation="2dp"
                android:background="#fff"
                android:clickable="true"
                android:focusable="true"
                android:onClick="visionInfo">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?attr/selectableItemBackground"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:id="@+id/arrow2"
                        android:src="@drawable/larrow"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentEnd="false"
                        android:layout_centerHorizontal="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Our Vision"
                        android:id="@+id/ourvision"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentRight="false"
                        android:layout_centerInParent="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center"
                        />

                </LinearLayout>
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:background="?attr/colorPrimary" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:elevation="2dp"
                android:background="#fff"
                android:clickable="true"
                android:focusable="true"
                android:onClick="visionInfo">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?attr/selectableItemBackground"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:id="@+id/arrow3"
                        android:src="@drawable/larrow"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentEnd="false"
                        android:layout_centerHorizontal="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Campus and Location"
                        android:id="@+id/campusLocation"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentRight="false"
                        android:layout_centerInParent="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center"
                        />

                </LinearLayout>
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:background="?attr/colorPrimary" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:elevation="2dp"
                android:background="#fff"
                android:clickable="true"
                android:focusable="true"
                android:onClick="contact">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="?attr/selectableItemBackground"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:id="@+id/arrow4"
                        android:src="@drawable/larrow"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentEnd="false"
                        android:layout_centerHorizontal="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Contact Us"
                        android:id="@+id/contact"
                        android:layout_alignParentStart="false"
                        android:layout_alignParentRight="false"
                        android:layout_centerInParent="true"
                        android:duplicateParentState="true"
                        android:layout_gravity="center"
                        />

                </LinearLayout>
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:background="?attr/colorPrimary" />

        </LinearLayout>
    </ScrollView>




</RelativeLayout>